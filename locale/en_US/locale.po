msgid "plugins.importexport.k10Plus.intro"
msgstr "The following information is required for successful deposit to the BSZ:"

msgid "plugins.importexport.k10Plus.settings.form.username"
msgstr "Username"

msgid "plugins.importexport.k10Plus.settings.form.folderId"
msgstr "Folder ID"

msgid "plugins.importexport.k10Plus.settings.form.usernameRequired"
msgstr "Please enter the username you got from the BSZ. The username may not contain colons."

msgid "plugins.importexport.k10Plus.settings.form.folderId.description"
msgstr "The folder path on the Server to which the deposited packages should be uploaded."

msgid "plugins.importexport.k10Plus.settings.form.serverAddress"
msgstr "Server Address"

msgid "plugins.importexport.k10Plus.settings.form.serverAddress.description"
msgstr "The Server address the deposited packages should be uploaded to."

msgid "plugins.importexport.k10Plus.settings.form.port"
msgstr "Port"

msgid "plugins.importexport.k10Plus.settings.form.port.description"
msgstr "The port the Server will be listening on."

msgid "plugins.importexport.k10Plus.settings.form.automaticDeposit.heading"
msgstr "Enable automatic deposit to the BSZ"

msgid "plugins.importexport.k10Plus.settings.form.automaticDeposit.description"
msgstr "OJS regularly checks if there are articles not yet deposited and deposits them automatically to the BSZ. Please note that this may take a short amount of time after publication to process. You can check for undeposited articles in the article listing."

msgid "plugins.importexport.k10Plus.statusLegend"
msgstr ""
"\n"
"\t\t<p>Deposit status:</p>\n"
"\t\t<p>\n <ul>"
"\t\t<li> Not deposited: no deposit attempt has been made for this article.</li>"
"\t\t<li> Active: the article has been deposited.</li>"
"\t\t<li> Failed: the article deposit has failed.</li>"
"\t\t<li> Marked active: the article was manually marked as active.</li>"
"\t\t<li> Marked inactive: the article was manually marked as inactive.</li>"
"\t\t</p>\n  </ul>"
"\t\t<p>Only the status of the last deposit attempt is displayed.</p>\n"
"\t\t<p>If a deposit has failed, please solve the problem and try to deposit again.</p>"

